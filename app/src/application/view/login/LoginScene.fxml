<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="116.0" prefWidth="341.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.view.login.LoginContr">
	<children>
		<Pane prefHeight="125.0" prefWidth="341.0">
			<children>
				<Button fx:id="fileSelectorBtn" layoutX="85.0" layoutY="77.0"
					mnemonicParsing="false" onAction="#fileSelected" text="文件选择" />
				<TextField fx:id="fileNameShowField" layoutX="14.0"
					layoutY="40.0" prefHeight="23.0" prefWidth="308.0" />
				<Label layoutX="14.0" layoutY="14.0" prefHeight="15.0"
					prefWidth="112.0" text="请选择打开文件:" />
				<Button fx:id="submitBtn" layoutX="185.0" layoutY="77.0"
					mnemonicParsing="false" onAction="#submit" text="打开" />
			</children>
		</Pane>
	</children>
</AnchorPane>
